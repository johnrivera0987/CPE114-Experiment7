#! /usr/local/iverilog/bin/vvp
:ivl_version "11.0 (devel)" "(s20150603-642-g3bdb50da)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "vhdl_textio";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00000000010dded0 .scope module, "random" "random" 2 1;
 .timescale 0 0;
v00000000010de1f0_0 .var "x", 3 0;
S_00000000010de060 .scope task, "borderclose" "borderclose" 2 12, 2 12 0, S_00000000010dded0;
 .timescale 0 0;
TD_random.borderclose ;
    %pushi/vec4 8, 0, 32;
T_0.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_0.1, 5;
    %jmp/1 T_0.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %vpi_call 2 15 "$display", "***                                              ***" {0 0 0};
    %jmp T_0.0;
T_0.1 ;
    %pop/vec4 1;
    %vpi_call 2 16 "$display", "****************************************************" {0 0 0};
    %end;
S_0000000000f46820 .scope task, "borderopen" "borderopen" 2 3, 2 3 0, S_00000000010dded0;
 .timescale 0 0;
TD_random.borderopen ;
    %vpi_call 2 5 "$display", "****************************************************" {0 0 0};
    %pushi/vec4 8, 0, 32;
T_1.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_1.3, 5;
    %jmp/1 T_1.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %vpi_call 2 8 "$display", "***                                              ***" {0 0 0};
    %jmp T_1.2;
T_1.3 ;
    %pop/vec4 1;
    %end;
    .scope S_00000000010dded0;
T_2 ;
    %vpi_call 2 22 "$monitor", "\011\011\011", v00000000010de1f0_0 {0 0 0};
    %delay 1, 0;
    %fork TD_random.borderopen, S_0000000000f46820;
    %join;
    %delay 1, 0;
    %vpi_call 2 24 "$display", "\011  This is a set of random values\012" {0 0 0};
    %pushi/vec4 4, 0, 32;
T_2.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.1, 5;
    %jmp/1 T_2.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 1, 0;
    %vpi_func 2 26 "$random" 32 {0 0 0};
    %pad/s 4;
    %store/vec4 v00000000010de1f0_0, 0, 4;
    %jmp T_2.0;
T_2.1 ;
    %pop/vec4 1;
    %delay 1, 0;
    %fork TD_random.borderclose, S_00000000010de060;
    %join;
    %delay 1, 0;
    %vpi_call 2 28 "$stop" {0 0 0};
    %delay 1, 0;
    %fork TD_random.borderopen, S_0000000000f46820;
    %join;
    %delay 1, 0;
    %vpi_call 2 30 "$display", "\011This is a new set of random values\012" {0 0 0};
    %pushi/vec4 4, 0, 32;
T_2.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.3, 5;
    %jmp/1 T_2.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 1, 0;
    %vpi_func 2 32 "$random" 32 {0 0 0};
    %pad/s 4;
    %store/vec4 v00000000010de1f0_0, 0, 4;
    %jmp T_2.2;
T_2.3 ;
    %pop/vec4 1;
    %delay 1, 0;
    %fork TD_random.borderclose, S_00000000010de060;
    %join;
    %delay 1, 0;
    %vpi_call 2 34 "$stop" {0 0 0};
    %delay 20, 0;
    %vpi_call 2 35 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
